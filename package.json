{
	"name": "ai-therapy",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev:frontend": "vite dev",
		"dev:emulate": "firebase emulators:start",
		"dev:functions": "cd functions && npm run build:watch",
		"dev": "run-p dev:frontend dev:emulate dev:functions",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"deploy": "firebase deploy --only hosting,functions"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^0.22.0",
		"clsx": "^2.1.1",
		"firebase-tools": "^13.29.1",
		"flowbite": "^2.5.2",
		"flowbite-svelte": "^0.47.4",
		"flowbite-svelte-icons": "^2.0.2",
		"fluid-tailwind": "^1.0.4",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.3.0",
		"tailwindcss": "^3.4.9",
		"typescript": "^5.0.0",
		"vite": "^5.4.11"
	},
	"dependencies": {
		"@fontsource-variable/jetbrains-mono": "^5.1.2",
		"@tailwindcss/typography": "^0.5.15",
		"firebase": "^11.1.0",
		"lucide-svelte": "^0.469.0"
	}
}